			"file": "feeds/foobar.rss",
			"itemregex": "<H3><A name=\"section[0-9]*\".*?>(.*?)</A></H3>.*?(<UL>(.*?)</UL>)",
			"maxitems": 30,
			"titleidx": 0,
			"linkidx": -1,
			"descidx": 1
